{
  "name": "typesafe-class-serializer",
  "version": "0.6.0",
  "description": "Class serialization and deserialization with ES6 decorators, typescript, and zod.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "check": "tsc --noEmit && eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkonowitch/typesafe-class-serializer.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "es6 decorators",
    "typescript",
    "zod",
    "serialization",
    "deserialization"
  ],
  "author": "Jeffrey Konowitch",
  "license": "MIT",
  "dependencies": {
    "immutable": "~4.3.5"
  },
  "peerDependencies": {
    "zod": ">3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@vitest/coverage-istanbul": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite-plugin-typescript-transpile": "~0.5.4",
    "vitest": "^1.4.0"
  }
}
